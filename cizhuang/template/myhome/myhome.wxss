.myhome-container{
    margin-bottom: 10rpx;
}
.userinfo {
  display: flex;
  flex-direction: column;
  padding: 50rpx 0;
  align-items: center;
  background: #f9f4f4;
}
.userinfo .avatar {
  width: 128rpx;
  height: 128rpx;
  margin: 20rpx;
  border-radius: 50%;
}
.userinfo .nickname {
  font-size: 18px;
  color: #666;
}
.navs{
    margin: 10rpx 0;
}
.nav{
    display: flex;
    align-items: center;
    padding: 30rpx;
    margin: 8rpx;
    border-bottom: 1px solid #dadada;
}
.nav image{
    width: 50rpx;
    height: 50rpx;
}
.nav text{
    margin-left: 30rpx;
    font-size: 18px;
    color: #555;
}